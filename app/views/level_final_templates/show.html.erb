<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Level final #<%= @level_final.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/level_finals" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link level_final start -->
      <div class="col">
        <a href="/level_finals/<%= @level_final.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit level final
        </a>
      </div>
      <!-- Edit link level_final end -->

      <!-- Delete link level_final start -->
      <div class="col">
        <a href="/delete_level_final/<%= @level_final.id %>" class="btn btn-block btn-outline-secondary">
          Delete level final
        </a>
      </div>
      <!-- Delete link level_final end -->
    </div>

    <dl>
      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@level_final.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@level_final.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->

<!-- Show Page Customization end -->
