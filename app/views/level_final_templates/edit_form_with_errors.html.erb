<% if @level_final.errors.any? %>
  <% @level_final.errors.full_messages.each do |message| %>
    <div class="alert">
      <%= message %>
    </div>
  <% end %>
<% end %>

<h1>
  Edit level final #<%= @level_final.id %>
</h1>

<hr>

<form action="/update_level_final/<%= @level_final.id %>" method="post">

  <button class="btn btn-block btn-outline-secondary">
    Update level final
  </button>
</form>

<hr>

<a href="/level_finals">
  Go back
</a>
