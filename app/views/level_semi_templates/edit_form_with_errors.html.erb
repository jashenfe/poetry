<!-- Validation failure messages -->
<% if @level_semi.errors.any? %>
  <% @level_semi.errors.full_messages.each do |message| %>
    <div class="alert">
      <%= message %>
    </div>
  <% end %>
<% end %>

<h1>
  Edit level semi #<%= @level_semi.id %>
</h1>

<hr>

<!-- Form level_semi start -->
<form action="/update_level_semi/<%= @level_semi.id %>" method="post">
<!-- Form level_semi end -->

  <button class="btn btn-block btn-outline-secondary">
    Update level semi
  </button>
</form>

<hr>

<a href="/level_semis">
  Go back
</a>
