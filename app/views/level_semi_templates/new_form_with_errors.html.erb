<!-- Form level_semi start -->
<form action="/create_level_semi" method="post">
<!-- Form level_semi end -->

  <button class="btn btn-block btn-outline-secondary">
    Create level semi
  </button>
</form>

<hr>

<a href="/level_semis">
  Go back
</a>
