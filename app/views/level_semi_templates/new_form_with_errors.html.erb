<!-- Validation failure messages -->
<% if @level_semi.present? %>
  <% if @level_semi.errors.any? %>
    <% @level_semi.errors.full_messages.each do |message| %>
      <div class="alert alert-danger">
        <%= message %>
      </div>
    <% end %>
  <% end %>
<% end %>

<h1>
