<% if @level_prelim.present? %>
  <% if @level_prelim.errors.any? %>
    <% @level_prelim.errors.full_messages.each do |message| %>
      <div class="alert alert-danger">
        <%= message %>
      </div>
    <% end %>
  <% end %>
<% end %>

<h1>
  Add a new level prelim
</h1>


<hr>

<form action="/create_level_prelim" method="post">

  <div class="form-group">
    <label for="bout_id">
      Bout
    </label>

    <input type="text" id="bout_id" name="bout_id" class="form-control" value="<%= @level_prelim.bout_id %>">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create level prelim
  </button>
</form>

<hr>

<a href="/level_prelims">
  Go back
</a>
