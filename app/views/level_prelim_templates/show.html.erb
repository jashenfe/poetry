<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Level prelim #<%= @level_prelim.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/level_prelims" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/level_prelims/<%= @level_prelim.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit level prelim
        </a>
      </div>

      <div class="col">
        <a href="/delete_level_prelim/<%= @level_prelim.id %>" class="btn btn-block btn-outline-secondary">
          Delete level prelim
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Bout
      </dt>
      <dd>
        <%= @level_prelim.bout_id %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@level_prelim.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@level_prelim.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

