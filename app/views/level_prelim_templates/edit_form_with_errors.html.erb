<% if @level_prelim.errors.any? %>
  <% @level_prelim.errors.full_messages.each do |message| %>
    <div class="alert">
      <%= message %>
    </div>
  <% end %>
<% end %>

<h1>
  Edit level prelim #<%= @level_prelim.id %>
</h1>

<hr>

<form action="/update_level_prelim/<%= @level_prelim.id %>" method="post">

  <div class="form-group">
    <label for="bout_id">
      Bout
    </label>

    <input type="text" id="bout_id" name="bout_id" class="form-control" value="<%= @level_prelim.bout_id %>">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Update level prelim
  </button>
</form>

<hr>

<a href="/level_prelims">
  Go back
</a>
