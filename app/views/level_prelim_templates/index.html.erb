<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All level prelims
    </h1>

    <a href="/level_prelims/new" class="btn btn-block btn-outline-secondary">
      Add a new level prelim
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Bout
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @level_prelims.each do |level_prelim| %>
      <tr>
        <!-- Display level_prelim.id start -->
        <td>
          <%= level_prelim.id %>
        </td>
        <!-- Display level_prelim.id end -->

        <!-- Display level_prelim.bout_id start -->
        <td>
          <%= level_prelim.bout_id %>
        </td>
        <!-- Display level_prelim.bout_id end -->

        <!-- Display level_prelim.created_at start -->
        <td>
          <%= time_ago_in_words(level_prelim.created_at) %> ago
        </td>
        <!-- Display level_prelim.created_at end -->

        <!-- Display level_prelim.updated_at start -->
        <td>
          <%= time_ago_in_words(level_prelim.updated_at) %> ago
        </td>
        <!-- Display level_prelim.updated_at end -->

        <td>
          <a href="/level_prelims/<%= level_prelim.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
