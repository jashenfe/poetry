<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All level prelims
    </h1>

    <a href="/level_prelims/new" class="btn btn-block btn-outline-secondary">
      Add a new level prelim
    </a>
  </div>
</div>

<div class="row">
  <div class="col-md-12 mb-3">
    <%= render partial: "level_prelim_templates/level_prelims_search_form" %>
  </div>
</div>



<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Bout
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @level_prelims.each do |level_prelim| %>
      <tr>
        <td>
          <%= level_prelim.id %>
        </td>

        <td>
          <%= level_prelim.bout_id %>
        </td>

        <td>
          <%= time_ago_in_words(level_prelim.created_at) %> ago
        </td>

        <td>
          <%= time_ago_in_words(level_prelim.updated_at) %> ago
        </td>

        <td>
          <a href="/level_prelims/<%= level_prelim.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @level_prelims, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
